      <!-- DataTales Example -->
      <div class="card shadow mb-4">
        <div class="card-header py-3">
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#add-modal">
                <i class="fas fa-plus"></i>
              </button>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>Nama</th>
                            <th>Email</th>
                            <th>Password</th>
                            <th>No hp</th>
                            <th>Jenis Kelamin</th>
                            <th>Action</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        <% for(let i = 0; i < users.length; i++){ %>
                            <tr>
                                <td><%= i + 1 %></td>
                                <td><%= users[i].name %></td>
                                <td><%= users[i].email %></td>
                                <td><%= users[i].password %></td>
                                <td><%= users[i].noHp %></td>
                                <td><%= users[i].jenisKelamin %></td>
                               
                                <td>
                                    <form action="/admin/users/<%= users[i].id %>?_method=DELETE" method="POST">
                                    <a href="javascript:void()" type="button" class="btn btn-warning btn-circle btn-sm button-update" 
                                        data-id="<%= users[i].id %>" 
                                        data-name="<%= users[i].name %>"
                                        data-email="<%= users[i].email %>"
                                        data-password="<%= users[i].password %>"
                                        data-nomorhp ="<%= users[i].noHp %>"
                                        data-jeniskelamin="<%= users[i].jenisKelamin %>"
                                         >
                                        <i class="fas fa-edit"></i>
                                    </a>
                                    <button type="submit" class="btn btn-danger btn-circle btn-sm" >
                                        <i class="fas fa-trash"></i>
                                    </button>
                                </form>
                                </td>
                            </tr>
                        <% } %>
                    </tbody>   
                </table>
            </div>
        </div>
    </div>