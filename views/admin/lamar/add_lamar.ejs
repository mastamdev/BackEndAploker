<div class="tab-pane fade" id="add-lamar" role="tabpanel" aria-labelledby="profile-tab">
  <form action="/admin/lamar" method="POST" enctype="multipart/form-data">
      <div class="modal-body">
              <div class="form-group">
                <div class="form-group">
                  <label for="name">Nama Users</label>
                  <select class="form-control" name="usersId">
                      <option value="">Nama Users</option>
                      <% for(let i= 0; i< users.length; i++){%> 
                        <option value="<%= users[i].id %>"><%= users[i].name %></option>
                      <%}%>
                  </select>
              </div>
          
                <label for="name">Nama Lengkap</label>
                <input type="text" class="form-control" name="namaLengkap" placeholder="Enter title" required>
              </div>
              <div class="form-group">
                <label for="name">Email</label>
                <input type="text" class="form-control" name="email" placeholder="Enter title" required>
              </div>
              <div class="form-group">
                <label for="name">No telepon</label>
                <input type="text" class="form-control" name="noHp" placeholder="Enter city" required>
              </div>
              <div class="form-group">
                  <label for="name">Pekerjaan</label>
                  <select class="form-control" name="jobId">
                      <option value="">Category</option>
                      <% for(let i= 0; i< job.length; i++){%> 
                        <option value="<%= job[i].id %>"><%= job[i].title %> || <%= job[i].company %></option>
                      <%}%>
                  </select>
              </div>
              
              <button type="submit" class="btn btn-primary">Save</button>
  </form>
</div>
</div>